digraph {
	node [align=center fontname=times fontsize=20 height=0.5 penwidth=2 shape=rect style=filled width=0.5]
	edge [fontname=times fontsize=20]
rankdir=LR
	"x_{t}" [fillcolor=darkseagreen2]
	"h_{t-1}" [fillcolor=darkseagreen2]
	0 [fillcolor=lightblue]
	"x_{t}" -> 0 [fillcolor=gray]
	"h_{t-1}" -> 0 [fillcolor=gray]
	1 [fillcolor=lightblue]
	2 [fillcolor=lightblue]
	3 [fillcolor=lightblue]
	4 [fillcolor=lightblue]
	5 [fillcolor=lightblue]
	0 -> 1 [label=relu fillcolor=gray]
	1 -> 2 [label=identity fillcolor=gray]
	2 -> 3 [label=identity fillcolor=gray]
	2 -> 4 [label=identity fillcolor=gray]
	3 -> 5 [label=identity fillcolor=gray]
	"h_{t}" [fillcolor=palegoldenrod]
	1 -> "h_{t}" [fillcolor=gray]
	2 -> "h_{t}" [fillcolor=gray]
	3 -> "h_{t}" [fillcolor=gray]
	4 -> "h_{t}" [fillcolor=gray]
	5 -> "h_{t}" [fillcolor=gray]
}
